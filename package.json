    "typescript": "^4.9.5",
    "wdio-chromedriver-service": "^8.1.1"
  },
  "scripts": {
    "wdio-android": "wdio run ./wdio.conf.android.js",
    "wdio-ios": "wdio run ./wdio.conf.ios.js",
    "wdio-browserstack-landscapes": "wdio run ./wdio.conf.browserstack-landscape.js",
    "wdio-browserstack-portrait": "wdio run ./wdio.conf.browserstack-portrait.js",
    "allure-report": "allure generate --clean && allure open",
